{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AzureAdB2C": {
    "Instance": "https://your-b2c-domain",
    "ClientId": "client-id-guid",
    "ClientSecret": "client-secret-guid",
    "TenantId": "tenant-guid",
    "Domain": "your-b2c-domain",
    "CallbackPath": "/signin-oidc",
    "SignedOutCallbackPath": "/signout-callback-oidc",
    "SignUpSignInPolicyId": "B2C_1_SignUpIn_Policy",
    "EditProfilePolicyId": "B2C_1_ProfileEdit_Policy",
    "ResetPasswordPolicyId": "B2C_1_PasswordReset_Policy",
    "AzureAdLogoutPath": "https://login.microsoftonline.com/common/oauth2/logout",
    "PostLogOutRedirectHost": "whatever-your-logic-url-is",
    "AccessDeniedPath": "/AccessDenied"
  }
}
